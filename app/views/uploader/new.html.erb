<%- title 'Upload photos' -%>

<%= @var %>

<%- form_tag(uploads_url, :id => "upload_form", :name => "upload_form", :target => "RSIFrame", :multipart => true) do -%>
<%= label_tag :photo %>
<%= file_field_tag :file_data, :onchange => "this.form.submit();" %>
<%- end -%>
<iframe name="RSIFrame" id="RSIFrame" style="width:0px;height:0px;border:none;"></iframe>
<%= link_to("call from server", "blank.html", :onclick => "callToServer();return false;") %>

