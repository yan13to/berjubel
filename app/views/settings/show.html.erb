<%- @page_title = User::Setting.model_name.human %>

<%= render_breadcrumbs([t(:home), User::Setting.model_name.human]) %>
<%= render_floating_alert([flash.now[:notice]].compact, :success) %>

<div class="card">
  <div class="card-body">
    <h5 class="card-title">
      <%= User::Setting.model_name.human %>
    </h5>
    <%= render 'setting_tabs' %>
    <div class="pt-3">
      <%= render 'setting', setting: @setting %>
    </div>
  </div>
</div>