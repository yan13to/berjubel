<%- @page_title = [t(:create), User::Profile.model_name.human].join(' | ') %>

<%= render_breadcrumbs([t(:home), t(:create), User::Profile.model_name.human]) %>

<div class="card">
  <div class="card-body">
    <h5 class="card-title">
      <%= User::Profile.model_name.human %>
    </h5>
    <div class="row">
      <div class="col-2">
        <%= render_profiles_avatar %>
      </div>
      <div class="col">
        <%= render 'profile_tabs' %>
        <div class="tab-content">
          <div class="pt-3>
            <%= render 'form' %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>